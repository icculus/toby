function main() returns nothing
    number i
	number color
	color = 0

    hideTurtle()

    for i = 1 to 360 step 2
		setPenColor(color)
        circle()
        turnRight(2)
		color = color + 1
		if (color > 11)
			color = 0
		endif
    endfor
endfunction


function circle() returns nothing
    number i

    for i = 1 to 360
        goForward(3)
        turnRight(1)
    endfor
endfunction



