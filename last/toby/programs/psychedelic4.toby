function main() returns nothing
    number i
	number angle = 0

    for i = 1 to 360 step 2
        circle()
		homeTurtle()
		angle = angle + 3
        turnRight(angle)
    endfor
endfunction


function circle() returns nothing
    number i
	number color

    for i = 1 to 360 step 4

		color = round(random() * 10) + 1
		setPenColor(color)
        goForward(3)
        turnRight(1)
    endfor
endfunction







